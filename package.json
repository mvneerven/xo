{
  "name": "@mvneerven/xo-js",
  "type": "module",
  "version": "1.3.9",
  "description": "ExoForm - Next Level Forms Generation based on JSON Schema",
  "repository": {
    "type": "git",
    "url": "https://github.com/mvneerven/xo"
  },
  "author": {
    "name": "Marc van Neerven",
    "email": "mvneerven@neerventure.com",
    "url": "https://neerventure.com/"
  },
  "keywords": [
    "spa",
    "es6",
    "json-schema",
    "vanilla-javascript",
    "javascript-library",
    "json-form",
    "no-dependencies",
    "inline-validation",
    "pwa-apps",
    "declarative-programming",
    "form-schema"
  ],
  "main": "dist/xo.min.js",
  "types": "dist/js/xo.d.ts",
  "homepage": "https://www.xo-js.dev",
  "scripts": {
    "dev": "esbuild js/xo.js --bundle --watch --outfile=dist/xo.js",
    "compile": "esbuild js/xo.js --bundle --minify --platform=node --target=node12 --format=esm --outfile=dist/xo.min.js&&tsc",
    "doc": "jsdoc src/exo src/pwa -r",
    "build": "npm run compile&&npm run test&&npm run doc",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "esbuild": "^0.11.23",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "regenerator-runtime": "^0.13.7",
    "typescript": "^4.2.4"
  }
}